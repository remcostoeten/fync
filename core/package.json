{
  "name": "@remcostoeten/fync-core",
  "version": "3.0.0",
  "description": "Core functionality for the fync monorepo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/remcostoeten/fync.git",
    "directory": "core"
  },
  "homepage": "https://github.com/remcostoeten/fync#readme",
  "bugs": {
    "url": "https://github.com/remcostoeten/fync/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "undici": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "undici": {
      "optional": true
    },
    "node-fetch": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "undici": "^6.0.0"
  }
}
